<% provide(:title, t(:all_users)) %>
<div class="dn-all-users">
<h1><%= t :all_users %></h1>

<%= will_paginate %>
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= image_tag user.avatar.url :medium %>
      <%= link_to user.name, user %>
    </li>
  <% end %>
</ul>
<%= will_paginate %>
</div>
